{
    "description": "Configuration for hjunit demo Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "300000",
        "bundle-name": "com.acts.example.dmsfwkstageserver",
        "package-name": "com.acts.example.dmsfwkstageserver",
        "shell-timeout": "600000",
        "testcase-timeout": 70000
    },
    "kits": [
        {
            "test-file-name": [
                "DctsDmsFaTest.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },
        {
            "type": "ShellKit",
            "run-command": [
                "power-shell wakeup",
                "power-shell setmode 602",
                "hidumper -s 3301 -a -t"
            ]
        }
    ]
}