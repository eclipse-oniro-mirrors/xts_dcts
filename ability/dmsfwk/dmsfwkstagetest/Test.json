{
    "description": "Configuration for capi ipc Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "300000",
        "shell-timeout": "300000",
        "testcase-timeout": 60000,
        "bundle-name": "com.acts.example.dmsfwkstagetest",
        "module-name": "entry_test"
    },
    "kits": [
        {
            "test-file-name": [
                "DctsDmsFwkStageTest.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },{
            "type": "ShellKit",
            "run-command": [
                "power-shell wakeup",
                "power-shell setmode 602"
            ]
        }
    ]
}
