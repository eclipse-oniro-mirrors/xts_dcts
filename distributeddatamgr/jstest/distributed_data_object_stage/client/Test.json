{
    "description": "Configuration for hjunit demo Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "180000",
        "shell-timeout": "600000",
        "bundle-name": "com.acts.distributedobjectdisets",
        "module-name": "entry_test",
        "testcase-timeout": 70000
    },
    "kits": [
        {
            "test-file-name": [
                "DctsDistributedObjectDisEtsTest.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },{
            "type": "ShellKit",
            "run-command": [
                "power-shell wakeup",
				"uinput -T -g 500 1200 500 700",
                "power-shell setmode 602"
            ]
        }
    ]
}