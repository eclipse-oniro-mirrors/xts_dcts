{
    "description": "Configuration for hjunit demo Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "180000",
	    "shell-timeout": "900000",
        "bundle-name": "com.acts.distributerdbdisets",
        "module-name": "entry_test",
        "testcase-timeout": 70000
    },
    "kits": [
        {
            "test-file-name": [
                "DctsDistributeRdbStoreDisEtsTest.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },{
            "type": "ShellKit",
            "run-command": [
                "power-shell wakeup",
				"power-shell setmode 602",
                "hidumper -s 3301 -a -t"
            ]
        }
    ] 
}
