{
    "description": "Configuration for hjunit demo Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "1500000",
        "bundle-name": "com.acts.device.manager.static",
        "module-name": "entry",
        "shell-timeout": "60000",
        "testcase-timeout": "50000"
    },
    "kits": [
        {
            "test-file-name": [
                "DctsDeviceManagerStaticTest.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },
        {
            "type": "ShellKit",
            "run-command": [
                "power-shell wakeup",
                "uinput -T -m 300 2700 280 200",
                "power-shell setmode 602"
            ],
            "teardown-command": [
                "power-shell setmode 600"
            ],
            "cleanup-apps": true
        }
    ]
}